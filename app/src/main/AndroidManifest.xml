<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.splash">

    <application
        android:allowBackup="true"
        android:icon="@drawable/encrypted"
        android:label="Passco"
        android:roundIcon="@drawable/encrypted"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".biometr"></activity>
        <activity android:name=".imagepicker" />
        <activity android:name=".editpass" />
        <activity android:name=".search" />
        <activity android:name=".profile" />
        <activity android:name=".addnewpassword" />
        <activity android:name=".mainpage" />
        <activity android:name=".register" />
        <activity
            android:name=".onboarding"
            android:theme="@style/Theme.Intro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>


    <uses-permission android:name="android.permission.INTERNET" />

</manifest>